FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 1e-5;

boundaryField
{
    left
    {
        type            fixedValue;
        value           uniform 1e-05;
    }

    right
    {
        type            inletOutlet;
        inletValue      uniform 1e-5;
        value           uniform 1e-5;
    }

    up
    {
        type            symmetryPlane;
    }

    down
    {
        type            symmetryPlane;
    }
    
    cylinder
    {
	type		kqRWallFunction;
	value		uniform 1e-5;
    }

    frontAndBack    
    {
        type            empty;
    }
}

// ************************************************************************* //
